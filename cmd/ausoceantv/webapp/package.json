{
  "name": "ausoceantv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --write \"**/*.{html,css,js,ts,json}\"",
    "format:watch": "onchange \"**/*.{html,css,js,ts,json}\" -- prettier --write --ignore-unknown {{changed}}",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "../../deploy.sh ausoceantv",
    "deploy:dev": "../../deploy.sh -db ausoceantv"
  },
  "dependencies": {
    "@lit/context": "^1.1.3",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "autoprefixer": "^10.4.20",
    "onchange": "^7.1.0",
    "postcss": "^8.4.48",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "vite": "^5.4.10"
  }
}
